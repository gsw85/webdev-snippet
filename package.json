    "react-dotenv": "^0.1.3",
    "react-firebase-hooks": "^5.1.1",
    "react-scripts": "5.0.1",
    "stripe": "^11.6.0",
    "uuid-v4": "^0.1.0",
    "web-vitals": "^2.1.4",
    "zustand": "^4.2.0"
